{
  "hooks": {
    "SessionStart": [
      {
        "matcher": "startup",
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/scripts/session-init.sh",
            "timeout": 10,
            "statusMessage": "Checking frontend-react-plugin configuration..."
          }
        ]
      }
    ]
  }
}
